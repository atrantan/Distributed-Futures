all: thesis report

thesis:
	latex thesis
	--bibtex thesis
	latex thesis
	dvipdfm thesis

report:	
	latex report
	--bibtex report
	latex report
	dvipdfm report

report:	
	latex report_extra
	--bibtex report_extra
	latex report_extra
	dvipdfm report_extra

clean:
	rm -rf *.aux *.bbl *.blg *.log *.out *.toc *.lof *.lot *.dvi *.pdf

clear:
	rm -rf *.aux *.bbl *.blg *.log *.out *.toc *.lof *.lot
