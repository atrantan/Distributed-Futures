set title "scalability over number of cores"
set xlabel "ncores"
set ylabel "execution time (ms)"
set yrange[0:]
set term postscript eps color font 12
set output "perfs/apps_scalability.eps"
plot "perfs/apps_scalability.dat" using 1:2 w linespoints title "lu, load=large", \
"perfs/apps_scalability.dat" using 1:3 w linespoints title "quicksort, load=large", \
"perfs/apps_scalability.dat" using 1:4 w linespoints title "fibonacci, load=large"
